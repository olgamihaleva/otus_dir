{
    "name": "hello_otus",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "babel-node src/main.js",
        "format": "npx prettier . --check",
        "format:fix": "npx prettier . --write",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "codestyle": "run-p lint format",
        "codestyle:fix": "run-s lint:fix format:fix",
        "build": "babel src --out-dir build --source-maps inline",
        "test": "jest --setupFiles dotenv/config --verbose",
        "test:ci": "cross-env CI=true NODE_OPTIONS=\"--max-old-space-size=2048\" jest",
        "coverage": "jest --coverage",
        "allure": "allure serve allure-results",
        "allure:ci": "allure generate allure-results --clean",
        "allure:ci:show": "allure open allure-report",
        "wdio": "wdio run ./wdio.conf.js",
        "codeceptjs": "codeceptjs run --steps",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "codeceptjs:ui": "codecept-ui --app",
        "codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples"
    },
    "author": "Olga Mihaleva",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.23.9",
        "@babel/node": "^7.23.9",
        "@babel/preset-env": "^7.23.8",
        "@codeceptjs/configure": "^1.0.1",
        "@codeceptjs/examples": "^1.2.4",
        "@codeceptjs/ui": "^1.1.1",
        "@faker-js/faker": "^8.4.0",
        "@jest/types": "^29.6.3",
        "@types/faker": "^6.6.9",
        "@types/jest": "^29.5.11",
        "@wdio/cli": "^8.35.1",
        "@wdio/local-runner": "^8.35.1",
        "@wdio/mocha-framework": "^8.35.0",
        "@wdio/spec-reporter": "^8.32.4",
        "allure-commandline": "^2.27.0",
        "allure-jest": "^2.12.2",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "codeceptjs": "^3.6.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.6.3",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.7.0",
        "npm-run-all": "^4.1.5",
        "playwright": "^1.43.1",
        "prettier": "3.1.1"
    },
    "dependencies": {
        "ajv": "^8.12.0",
        "axios": "^1.6.7",
        "dotenv": "^16.4.1",
        "faker": "^5.5.3",
        "form-data": "^4.0.0",
        "jasmine-allure-reporter": "^1.0.2",
        "jest-allure": "^0.1.3",
        "jest-jasmine2": "^29.7.0",
        "node-fetch": "^2.7.0"
    }
}
